import java.util.Random;

public class WQ9 {
  public static void main(String[] args) {
    Random rand = new Random();
    int n[ ] = new int[10];
  
    
    
    for(int i = 0; i < 1000; i++) {
      ++n[rand.nextInt(10)];
    }
    for(int max = 0; max < 100; max++) {
    System.out.println(n[max]);
  }
    for(int min = 0; min < 50; min--) {
      System.out.println(n[min];
  }
}