import java.util.Random;

public class WQ8 {
  public static void main(String[] args) {
    Random rand = new Random();
    int n[] = new int[10];
  
    
    
    for(int i = 0; i < 1000; i++) {
      ++n[rand.nextInt(10)];
    }
    for(int max = 0; max < 100; max++) {
    System.out.println(n[max]);
  }
}
}

